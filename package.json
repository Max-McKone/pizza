{
  "name": "pizza",
  "version": "0.0.0",
  "description": "Pizza!",
  "main": "build",
  "scripts": {
    "start": "node build/server",
    "build-client": "webpack -p --module-bind 'json=json-loader' build/client build/client/bundle.js",
    "build-babel": "babel source -d build",
    "build-copy": "cp source/client/index.html build/client/index.html",
    "build": "npm run -s build-babel && npm run -s build-client && npm run -s build-copy",
    "watch": "nodemon --watch package.json --watch menu.json --watch source -e js,json,html,css,sass,png,jpg --exec \"npm run -s build && npm run -s start\""
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Max-Moolato/pizza.git"
  },
  "keywords": [
    "pizza"
  ],
  "author": "S.H.I.E.L.D.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Max-Moolato/pizza/issues"
  },
  "homepage": "https://github.com/Max-Moolato/pizza#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "hyperapp": "0.0.7",
    "nedb": "^1.8.0",
    "spirit": "^0.4.0",
    "spirit-body": "0.0.3",
    "spirit-router": "^0.4.0",
    "superagent": "^3.4.1"
  }
}
